let mode = "normal";   является корректным
let my_valu3 = 102;   является корректным
let 3my_value3 = "102";   не является корректным, так как имя начинается с цифры
let __hello__ = "world";   является корректным
let $$$ = "money";   является корректным
let !0_world = true;   не является корректным, так как имя начинается со спецсимвола